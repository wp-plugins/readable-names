#: readable-names.php:89
msgid "Error: The name is not readable"
msgstr "Virhe: Nimi ei ole luettavassa muodossa."

#: readable-names.php:143
#, php-format
msgid "<strong>Error:</strong> The name “%1$s” contains an invalid character: “%2$s”. Please only use these characters: “%3$s”."
msgstr "<strong>Error:</strong> Nimi “%1$s” sisältää virheellisen merkin: “%2$s”. Käytäthän vain seuraavia merkkejä: “%3$s”."

#: readable-names.php:162
#, php-format
msgid "<strong>Error:</strong> The name “%1$s” is too short. It has to be at least %2$d characters long."
msgstr "<strong>Virhe:</strong> Nimi “%1$s” on liian lyhyt. Nimen täytyy olla vähintään %2$d merkkiä pitkä."

#: readable-names.php:192
#, php-format
msgid "<strong>Error:</strong> The name “%s” does not begin with a capital letter."
msgstr "<strong>Virhe:</strong> Nimi “%s” ei ala isolla kirjaimella."

#: readable-names.php:210
#, php-format
msgid "<strong>Error:</strong> The name “%s” has to many capital letters."
msgstr "<strong>Virhe:</strong> Nimi “%s” sisältää monia isoja kirjaimia."

