#: readable-names.php:86
msgid "Error: non readable name"
msgstr "Fehler: nicht lesbarer Name"

#: readable-names.php:137
#, php-format
msgid "Name &ldquo;%s&rdquo; contains an invalid character &ldquo;%s&rdquo;."
msgstr "Der Name &raquo;%s&laquo; enthält ein unzulässiges Zeichen &raquo;%s&laquo;."

#: readable-names.php:155
#, php-format
msgid "Name &ldquo;%s&rdquo; does not contain required letters &ldquo;%s&rdquo;."
msgstr "Der Name &raquo;%s&laquo; enthält nicht die erforderlichen Buchstaben &raquo;%s&laquo;."

#: readable-names.php:173
#, php-format
msgid "Name &ldquo;%s&rdquo; is too short. It has to be at least %d characters long."
msgstr "Der Name &raquo;%s&laquo; ist zu kurz. Er sollte mindestens %d Zeichen lang sein."

#: readable-names.php:203
#, php-format
msgid "Name &ldquo;%s&rdquo; does not begin with a capital letter."
msgstr "Der Name &raquo;%s&laquo; beginnt nicht mit einem Großbuchstaben."

#: readable-names.php:221
#, php-format
msgid "Name &ldquo;%s&rdquo; has to many capital letters."
msgstr "Der Name &raquo;%s&laquo; enthält zu viele Großbuchstaben."

#: readable-names.php:263
msgid "Readable Names Options"
msgstr "Einstellungen für »Lesbare Namen«"

#. Plugin Name of the plugin/theme
#: readable-names.php:264 readable-names.php:273
msgid "Readable Names"
msgstr "Lesbare Namen"

#: readable-names.php:289
msgid "Allowed characters"
msgstr "Erlaubte Zeichen"

#: readable-names.php:290
msgid "Small letters"
msgstr "Kleinbuchstaben"

#: readable-names.php:291
msgid "Capital letters"
msgstr "Großbuchstaben"

#: readable-names.php:292
msgid "Required letters"
msgstr "Erforderliche Buchstaben"

#: readable-names.php:293
msgid "Digits"
msgstr "Ziffern"

#: readable-names.php:296
msgid "Rules"
msgstr "Regeln"

#: readable-names.php:297
msgid "Minimum name length"
msgstr "Mindestlänge des Namens"

#: readable-names.php:298
msgid "First character must be a capital letter"
msgstr "Der erste Buchstabe muss ein Großbuchstabe sein"

#: readable-names.php:299
msgid "One capital letter only"
msgstr "Nur ein Großbuchstabe"

#: readable-names.php:303
msgid "Whitespace is always allowed."
msgstr "Das Leerzeichen ist immer erlaubt."

#: readable-names.php:351
msgid "Depending on allowed characters."
msgstr "In Abhängigkeit von den erlaubten Zeichen."

#: readable-names.php:362
msgid "characters"
msgstr "Zeichen"

