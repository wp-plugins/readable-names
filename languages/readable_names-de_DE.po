#: readable-names.php:93
msgid "Error: The name is not readable"
msgstr "Fehler: Der Name ist nicht lesbar"

#: readable-names.php:150
#, php-format
msgid "<strong>Error:</strong> The name “%1$s” contains an invalid character: “%2$s”. Please only use these characters: “%3$s”."
msgstr "<strong>Fehler:</strong> Der Name »%1$s«; enthält ein unzulässiges Zeichen »;%2$s«. Bitte nur diese Zeichen verwenden: »%3$s«."

#: readable-names.php:169
#, php-format
msgid "<strong>Error:</strong> The name “%1$s” is too short. It has to be at least %2$d characters long."
msgstr "<strong>Fehler:</strong> Der Name »%1$s« ist zu kurz. Er sollte mindestens %2$d Zeichen lang sein."

#: readable-names.php:199
#, php-format
msgid "<strong>Error:</strong> The name “%s” does not begin with a capital letter."
msgstr "<strong>Fehler:</strong> Der Name »%s« beginnt nicht mit einem Großbuchstaben."

#: readable-names.php:217
#, php-format
msgid "<strong>Error:</strong> The name “%s” has to many capital letters."
msgstr "<strong>Fehler:</strong> Der Name »%s« enthält zu viele Großbuchstaben."

#: readable-names.php:238
#, php-format
msgid "<strong>Error:</strong> The name “%1$s” does not contain required vowels: “%2$s”."
msgstr "<strong>Fehler:</strong> Der Name »%1$s« enthält nicht die erforderlichen Vokale: »%2$s«."

