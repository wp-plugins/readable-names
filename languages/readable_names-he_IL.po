#: readable-names.php:93
msgid "Error: The name is not readable"
msgstr "שגיאה: השם אינו קריא"

#: readable-names.php:150
#, php-format
msgid ""
"<strong>Error:</strong> The name “%1$s” contains an invalid character: “%2"
"$s”. Please only use these characters: “%3$s”."
msgstr ""
"<strong>שגיאה:</strong> השם “%1$s” מכיל את התו הלא חוקי: “%2$s”. נא להשתמש "
"בתווים אלו בלבד: “%3$s”."

#: readable-names.php:169
#, php-format
msgid ""
"<strong>Error:</strong> The name “%1$s” is too short. It has to be at least %"
"2$d characters long."
msgstr ""
"<strong>שגיאה:</strong> השם “%1$s” קצר מדי. עליו להיות בן %2$d תווים לפחות."

#: readable-names.php:199
#, php-format
msgid ""
"<strong>Error:</strong> The name “%s” does not begin with a capital letter."
msgstr "<strong>שגיאה:</strong> השם “%s” אינו מתחיל באות גדולה."

#: readable-names.php:217
#, php-format
msgid "<strong>Error:</strong> The name “%s” has to many capital letters."
msgstr "<strong>שגיאה:</strong> בשם “%s” יש יותר מדי אותיות גדולות."

#: readable-names.php:238
#, php-format
msgid ""
"<strong>Error:</strong> The name “%1$s” does not contain required vowels: “%2"
"$s”."
msgstr ""
"<strong>שגיאה:</strong> השם “%1$s” אינו מכיל את אמות הקריאה הנדרשות: “%2$s”."

